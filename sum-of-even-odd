#include<iostream>
using namespace std;

int main() {
	// Write your code here
	int n;
	cin>>n;
	int even,odd;
	even = 0;
	odd= 0;
	while(n!=0){
		int mod;
		mod = n%10;
		if (mod%2==0){
			even= even + mod;
		}
		else if (mod%2 !=0){
			odd = odd + mod;

		}
		n=n/10;
	}
	cout<<even<<"  "<<odd<<endl;
	
}

